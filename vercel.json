{
  "version": 2,
  "functions": {
    "server.js": {
      "includeFiles": [
        "views/**",
        "data/**",
        "modules/**",
        "public/**"
      ]
    }
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}